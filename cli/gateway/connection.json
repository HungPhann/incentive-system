        "mychannel": {
            "orderers": [
                "orderer.example.com"
            ],
            "peers": {
                "peer0.org1.example.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer0.org2.example.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                }
            },
            "chaincodes": [
                "contract_0:1.0",
                "company1_contract:1.0",
                "company2_contract:1.0"
            ]
        }
    },
    "organizations": {
        "Org0": {
            "mspid": "Org0MSP",
            "peers": [
            ],
            "certificateAuthorities": [
                "ca.org0.example.com"
            ]
        },
        "Org1": {
            "mspid": "Org1MSP",
            "peers": [
                "peer0.org1.example.com"
            ],
            "certificateAuthorities": [
            ]
        },"Org2": {
            "mspid": "Org2MSP",
            "peers": [
                "peer0.org2.example.com"
            ],
            "certificateAuthorities": [
            ]
        }
    },
    "orderers": {
